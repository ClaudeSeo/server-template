{
  "name": "server-template",
  "version": "0.0.0",
  "description": "Server Template",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "node",
    "typescript",
    "nestjs",
    "boilerplate"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ClaudeSeo/server-template.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "dev": "nest start",
    "dev:watch": "nest start --watch",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "test": "jest",
    "gen:docs": "nest start --config nest-cli-doc.json",
    "docs": "redoc-cli serve ./dist/spec.json"
  },
  "author": {
    "name": "Claude.Seo",
    "email": "ehdaudtj@gmail.com",
    "url": "https://clud.me"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/terminus": "^7.0.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.2",
    "nestjs-pino": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-pretty": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/testing": "^7.6.18",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.4",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-security": "^1.4.0",
    "gts": "^3.1.0",
    "jest": "^27.0.6",
    "pkginfo": "^0.4.1",
    "redoc-cli": "^0.12.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^1.2.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "rootDir": "./src",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/$1"
    },
    "testEnvironment": "node"
  }
}
